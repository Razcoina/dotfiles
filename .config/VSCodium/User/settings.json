{
	"files.autoSave": "afterDelay",
	"editor.formatOnSave": true,
	"notebook.formatOnSave.enabled": true,
	"editor.insertSpaces": false,
	"editor.guides.bracketPairs": "active",
	"editor.indentSize": "tabSize",
	"editor.linkedEditing": true,
	"editor.mouseWheelZoom": true,
	"editor.stablePeek": true,
	"diffEditor.codeLens": true,
	"editor.acceptSuggestionOnEnter": "off",
	"editor.inlineSuggest.showToolbar": "always",
	"editor.inlineSuggest.suppressSuggestions": true,
	"editor.quickSuggestionsDelay": 0,
	"editor.suggest.localityBonus": true,
	"editor.suggest.preview": true,
	"editor.suggest.shareSuggestSelections": true,
	"editor.suggest.showStatusBar": true,
	"editor.suggest.snippetsPreventQuickSuggestions": true,
	"files.insertFinalNewline": true,
	"files.readonlyFromPermissions": true,
	"workbench.accounts.experimental.showEntitlements": true,
	"workbench.settings.useSplitJSON": true,
	"window.confirmBeforeClose": "keyboardOnly",
	"window.density.editorTabHeight": "compact",
	"search.showLineNumbers": true,
	"debug.allowBreakpointsEverywhere": true,
	"debug.showBreakpointsInOverviewRuler": true,
	"testing.showAllMessages": true,
	"scm.alwaysShowRepositories": true,
	"scm.alwaysShowActions": true,
	"terminal.integrated.allowMnemonics": true,
	"terminal.integrated.accessibleViewFocusOnCommandExecution": true,
	"terminal.integrated.accessibleViewPreserveCursorPosition": true,
	"terminal.integrated.defaultProfile.linux": "zsh",
	"terminal.integrated.enableImages": true,
	"terminal.integrated.gpuAcceleration": "on",
	"terminal.integrated.profiles.linux": {
		"bash": {
			"path": "bash",
			"icon": "terminal-bash"
		},
		"zsh": {
			"path": "zsh"
		},
		"fish": {
			"path": "fish"
		},
		"tmux": {
			"path": "tmux",
			"icon": "terminal-tmux"
		},
		"pwsh": {
			"path": "pwsh",
			"icon": "terminal-powershell"
		}
	},
	"terminal.integrated.sendKeybindingsToShell": true,
	"css.format.spaceAroundSelectorSeparator": true,
	"less.format.spaceAroundSelectorSeparator": true,
	"scss.format.spaceAroundSelectorSeparator": true,
	"scss.lint.float": "warning",
	"less.lint.float": "warning",
	"git.showPushSuccessNotification": true,
	"git.supportCancellation": true,
	"html.format.indentHandlebars": true,
	"html.format.indentInnerHtml": true,
	"Lua.codeLens.enable": true,
	"Lua.hint.setType": true,
	"python.terminal.focusAfterLaunch": true,
	"editor.wordBasedSuggestions": "allDocuments",
	"better-comments.highlightPlainText": true,
	"path-intellisense.autoSlashAfterDirectory": true,
	"workbench.iconTheme": "material-icon-theme",
	"editor.semanticHighlighting.enabled": true,
	"accessibility.signals.terminalBell": {
		"sound": "on"
	},
	"terminal.integrated.enableVisualBell": true,
	"editor.renderWhitespace": "all",
	"grunt.autoDetect": "on",
	"gulp.autoDetect": "on",
	"jake.autoDetect": "on",
	"workbench.startupEditor": "none",
	"terminal.external.linuxExec": "konsole",
	"editor.stickyScroll.enabled": false,
	"clangd.checkUpdates": true,
	"path-intellisense.showHiddenFiles": true,
	"fiveServer.port": 8080,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"notebook.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.debug": true,
	"eslint.lintTask.enable": true
}
