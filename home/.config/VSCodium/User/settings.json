{
  "telemetry.enableTelemetry": false,
  "telemetry.feedback.enabled": false,
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "files.readonlyFromPermissions": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.indentSize": "tabSize",
  "editor.inlineSuggest.showToolbar": "always",
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.linkedEditing": true,
  "editor.mouseWheelZoom": true,
  "editor.quickSuggestionsDelay": 0,
  "editor.renderWhitespace": "all",
  "editor.semanticHighlighting.enabled": true,
  "editor.stablePeek": true,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.suggest.showStatusBar": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": "allDocuments",
  "editor.wordWrap": "on",
  "notebook.defaultFormatter": "esbenp.prettier-vscode",
  "notebook.formatOnSave.enabled": true,
  "diffEditor.codeLens": true,
  "workbench.colorTheme": "One Dark Modern Pro",
  "workbench.preferredDarkColorTheme": "One Dark Modern Pro",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "none",
  "window.confirmBeforeClose": "keyboardOnly",
  "window.density.editorTabHeight": "compact",
  "search.showLineNumbers": true,
  "debug.allowBreakpointsEverywhere": true,
  "debug.showBreakpointsInOverviewRuler": true,
  "testing.showAllMessages": true,
  "scm.alwaysShowActions": true,
  "scm.alwaysShowRepositories": true,
  "terminal.integrated.allowMnemonics": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.enableImages": true,
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh"
    },
    "fish": {
      "path": "fish"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.external.linuxExec": "/usr/bin/konsole",
  "chat.editor.wordWrap": "on",
  "accessibility.signals.terminalBell": {
    "sound": "on"
  },
  "css.format.enable": false,
  "css.lint.unknownAtRules": "ignore",
  "less.format.enable": false,
  "less.lint.float": "warning",
  "scss.format.enable": false,
  "scss.lint.float": "warning",
  "git.autofetch": "all",
  "git.confirmSync": false,
  "git.defaultBranchName": "master",
  "git.enableCommitSigning": true,
  "git.fetchOnPull": true,
  "git.showPushSuccessNotification": true,
  "html.format.enable": false,
  "html.format.wrapLineLength": 80,
  "Lua.codeLens.enable": true,
  "Lua.format.defaultConfig": {
    "max_line_length": "80"
  },
  "Lua.hint.setType": true,
  "python.terminal.focusAfterLaunch": true,
  "basedpyright.analysis.inlayHints.callArgumentNames": false,
  "basedpyright.analysis.inlayHints.functionReturnTypes": false,
  "basedpyright.analysis.inlayHints.variableTypes": false,
  "better-comments.highlightPlainText": true,
  "path-intellisense.absolutePathToWorkspace": true,
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.ignoreTsConfigBaseUrl": true,
  "path-intellisense.showHiddenFiles": true,
  "grunt.autoDetect": "on",
  "gulp.autoDetect": "on",
  "jake.autoDetect": "on",
  "clangd.checkUpdates": true,
  "fiveServer.php.executable": "/usr/bin/php",
  "fiveServer.php.ini": "/etc/php/php.ini",
  "fiveServer.port": 8080,
  "prettier.enableDebugLogs": true,
  "prettier.requireConfig": true,
  "prettier.resolveGlobalModules": true,
  "eslint.debug": true,
  "eslint.lintTask.enable": true,
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "dart.devToolsBrowser": "default",
  "dart.showExtensionRecommendations": false,
  "dart.flutterSdkPath": "~/Android/flutter",
  "php.debug.executablePath": "/usr/bin/php",
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "phpunit.phpunit": "vendor/bin/pest",
  "intelephense.codeLens.implementations.enable": true,
  "intelephense.codeLens.overrides.enable": true,
  "intelephense.codeLens.parent.enable": true,
  "intelephense.codeLens.references.enable": true,
  "intelephense.codeLens.usages.enable": true,
  "intelephense.files.exclude": ["**/.git/**", "**/node_modules/**"],
  "intelephense.format.enable": false,
  "intelephense.stubs": [
    "apache",
    "bcmath",
